{
    "name": "vse/browsehappy",
    "type": "phpbb-extension",
    "description": "Displays an Outdated Browser Banner on your forum's index page (visible only to Internet Explorer 6/7/8 users), to encourage users of IE 6/7/8 to upgrade to a modern browser.",
    "homepage": "https://github.com/VSEphpbb/browsehappy",
    "version": "1.1.0",
    "time": "2013-11-09",
    "licence": "GPL-2.0",
    "authors": [
        {
            "name": "Matt Friedman (VSE)",
            "homepage": "http://mattfriedman.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": "3.1.*@dev"
    },
    "extra": {
        "display-name": "Browse Happy"
    }
}
